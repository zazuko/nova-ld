  rr:subjectMap [
    rr:template "http://lod.opentransportdata.swiss/nova/kante/{@id}" ;
    rr:class nova:Kante ;
    rr:termType rr:IRI ;
  ] ;

 rr:predicateObjectMap [
    rr:predicate dc:identifier;
    rr:objectMap [
      rml:reference "@id";
    ]
 ] ;
  
  rr:predicateObjectMap [
    rr:predicate nova:from;
    rr:objectMap [
      rml:reference "version[1]/haltestelle1";
    ]
 ] ;

   rr:predicateObjectMap [
    rr:predicate nova:to;
    rr:objectMap [
      rml:reference "version[1]/haltestelle2";
    ]
 ] ;
 
    rr:predicateObjectMap [
    rr:predicate nova:verwaltung;
    rr:objectMap [
      rml:reference "version[1]/verwaltung";
    ]
 ] ;

    rr:predicateObjectMap [
    rr:predicate nova:verkehrsmittelTyp;
    rr:objectMap [
      rml:reference "version[1]/verkehrsmittelTyp";
    ]
 ] ;

    rr:predicateObjectMap [
    rr:predicate nova:zuschlagspflichtig;
    rr:objectMap [
      rml:reference "version[1]/zuschlagspflichtig";
      rr:datatype xsd:boolean ;
    ]
 ] ;

    rr:predicateObjectMap [
    rr:predicate nova:bahnersatz;
    rr:objectMap [
      rml:reference "version[1]/bahnersatz";
      rr:datatype xsd:boolean ;
    ]
 ] .

