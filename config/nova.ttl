@base <http://lod.opentransportdata.swiss/nova/> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix carml: <http://carml.taxonic.com/carml/> .
@prefix nova:   <http://vocab.nova.com#> .
@prefix sem: <http://semanticweb.cs.vu.nl/2009/11/sem/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<#Partner> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
      a carml:Stream ;
	   carml:declaresNamespace [
        carml:namespacePrefix "ns2" ;
        carml:namespaceName "ch.voev.nova.pflege.common.exporter.datenrelease" ;
      ] ;
    ] ;
    rml:referenceFormulation ql:XPath ;
    rml:iterator "/ns2:datenrelease/subsystemFQF/partners/partner" ;
  ] ;

  rr:subjectMap [
    rr:template "http://lod.opentransportdata.swiss/nova/partner/{@id}" ;
    rr:class nova:Partner ;
    rr:termType rr:IRI ;
  ] ;
  
  rr:predicateObjectMap [
    rr:predicate nova:partnerCode;
    rr:objectMap [
      rml:reference "partnerCode";
    ]
 ] ;
  
  rr:predicateObjectMap [
    rr:predicate nova:partnerKuerzel;
    rr:objectMap [
      rml:reference "partnerKuerzel";
    ]
 ] ;
 
 rr:predicateObjectMap [
    rr:predicate dc:identifier;
    rr:objectMap [
      rml:reference "@id";
    ]
 ] ;
  
  rr:predicateObjectMap [
    rr:predicate rdfs:label;
    rr:objectMap [
      rml:reference "druckBezeichnung/de";
    ]
 ] .

 <#Kante> a rr:TriplesMap ;
  rml:logicalSource [
    rml:source [
      a carml:Stream ;
	   carml:declaresNamespace [
        carml:namespacePrefix "ns2" ;
