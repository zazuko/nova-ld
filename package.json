{
  "name": "ld-didok",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:zazuko/ld-didok.git"
  },
  "dependencies": {
    "@rmlio/yarrrml-parser": "^0.2.1",
    "barnard59": "git+https://github.com/zazuko/barnard59.git#master",
    "json": "^9.0.6",
    "npm-run-all": "^4.1.2"
  },
  "devDependencies": {},
  "scripts": {
    "clean": "rm -rf target/*",
    "fetch-carml": "mkdir -p tmp/lib && curl -L https://github.com/netage/carml-cli/releases/download/cli-0.0.6/cli-0.0.6-SNAPSHOT-jar-with-dependencies.jar -o tmp/lib/carml-cli.jar",
    "convert-nova": "mkdir -p target && java -jar tmp/lib/carml-cli.jar -i tmp/datenrelease_96.20180920-2201.P.P.4596.0.xml -m config/nova.ttl -of nt -o target/nova.nt",
    "tdb-load": "tdbloader --loc=target/tdb --graph=http://example.com/nova input/schema.ttl target/void.ttl target/nova.nt",
    "tdb-query": "./scripts/tdb-query.sh",
    "tdb-dump": "./scripts/tdb-dump.sh",
    "validate-riot": "riot --validate target/everything.nt.gz",
    "void-timestamp": "./scripts/void-timestamp.sh",
    "didok-integ-s3": "ENDPOINT=http://data.zazuko.com/lindas STORE_USER=$STORE_USER_INTEG STORE_PASSWORD=$STORE_PASSWORD_INTEG ./scripts/store-s3.sh",
    "didok-prod-s3": "ENDPOINT=https://lindas-data.ch:8443/lindas STORE_USER=$STORE_USER_PROD STORE_PASSWORD=$STORE_PASSWORD_PROD ./scripts/store-s3.sh"
  }
}
